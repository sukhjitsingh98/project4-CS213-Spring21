<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.collections.FXCollections?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.OrderingCoffeeController">
   <children>
      <HBox layoutX="14.0" layoutY="123.0" prefHeight="25.0" prefWidth="383.0">
         <children>
            <CheckBox id="creamCheckBox" mnemonicParsing="false" prefHeight="17.0" prefWidth="65.0" text="Cream" />
            <CheckBox id="syrupCheckBox" mnemonicParsing="false" prefHeight="17.0" prefWidth="59.0" text="Syrup" />
            <CheckBox id="milkCheckBox" mnemonicParsing="false" prefHeight="17.0" prefWidth="59.0" text="Milk" />
            <CheckBox id="caramelCheckBox" mnemonicParsing="false" prefHeight="17.0" prefWidth="74.0" text="Caramel" />
            <CheckBox id="whippedCreamCheckBox" mnemonicParsing="false" text="Whipped Cream" />
         </children>
      </HBox>

      <ComboBox fx:id="donutCombo" layoutX="14.0" layoutY="33.0" prefHeight="25.0" prefWidth="123.0" promptText="Donut Type">
         <items>
            <FXCollections fx:factory="observableArrayList">
               <String fx:value="Yeast Donuts" />
               <String fx:value="Cake Donuts" />
               <String fx:value="Donut Holes" />
            </FXCollections>
         </items>
      </ComboBox>
   </children>
</AnchorPane>
