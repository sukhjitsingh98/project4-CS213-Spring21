<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.OrderingDonutsController">
   <children>
      <ComboBox fx:id="combobox" layoutX="22.0" layoutY="57.0" onAction="#handleDonutSelection" prefWidth="150.0" promptText="Select Donut " />
      <ListView fx:id="selectableDonuts" layoutX="20.0" layoutY="182.0" prefHeight="157.0" prefWidth="226.0" />
      <ListView fx:id="selectedDonuts" layoutX="352.0" layoutY="182.0" prefHeight="157.0" prefWidth="226.0" />
      <Button fx:id="addDonut" layoutX="274.0" layoutY="236.0" mnemonicParsing="false" onAction="#handleDonutAddition" prefHeight="25.0" prefWidth="52.0" text="-&gt;" />
      <Button fx:id="removeDonut" layoutX="274.0" layoutY="291.0" mnemonicParsing="false" onAction="#handleDonutRemoval" prefHeight="25.0" prefWidth="52.0" text="&lt;-" />
      <ComboBox fx:id="countCombobox" layoutX="267.0" layoutY="182.0" prefHeight="25.0" prefWidth="66.0" />
      <Button fx:id="addOrder" layoutX="385.0" layoutY="105.0" mnemonicParsing="false" onAction="#handleAddToOrder" text="add to order " />
      <Text layoutX="256.0" layoutY="135.0" strokeType="OUTSIDE" strokeWidth="0.0" />
      <Label fx:id="noSelectionWarning1" layoutX="22.0" layoutY="136.0" prefHeight="36.0" prefWidth="554.0" textAlignment="CENTER" />
      <HBox layoutX="342.0" layoutY="65.0" prefHeight="17.0" prefWidth="157.0">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Sub-Total:" wrappingWidth="66.0" />
            <Label fx:id="subTotalLabel" prefHeight="17.0" prefWidth="91.0" />
         </children>
      </HBox>
   </children>
</AnchorPane>
